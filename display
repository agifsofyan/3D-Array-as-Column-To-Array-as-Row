      "periode" => "Ags'16A"
      "frek_debet" => "A1"
    ]
  ]
  1 => array:7 [
    0 => array:2 [
      "periode" => "Apr'16B"
      "frek_debet" => "B1"
    ]
    1 => array:2 [
      "periode" => "Mei'16B"
      "frek_debet" => "B24"
